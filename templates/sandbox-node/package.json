{
  "name": "sandbox-node",
  "description": "Minimal template for quick experimenting with TypeScript and Node.js",
  "license": "MIT",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "start:watch": "tsx --watch ./src/index.ts",
    "check-types": "tsc",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "type": "module",
  "prettier": "@socnik/my-prettier-config",
  "packageManager": "pnpm@8.15.6",
  "devDependencies": {
    "@socnik/my-prettier-config": "^1.0.3",
    "@types/node": "^20.12.8",
    "@vue/tsconfig": "^0.5.1",
    "prettier": "^3.2.5",
    "tsx": "^4.8.2",
    "typescript": "^5.4.5"
  }
}
